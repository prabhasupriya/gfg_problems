int nextPrime(int n) {

    // code here to find next prime number
    // return next prime number
    for(int i=n+1; i<= 2*n ;i++){
        bool found =true;
        for(int j =2;j<= sqrt(i);j++){
            if(i%j == 0){
                found = false;
                break;
            }
        }
        if(found){
            return  i;
            break;
        }
    }
};
