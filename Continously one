class Solution {
public:
    int findMaxConsecutiveOnes(vector<int>& nums) {
        int count =0;
        int current =0;
        for(int i=0;i<nums.size();i++){
            if(nums[i] == 1){
                current = current +1;
                count = max(current, count);
            }
            else{
                current =0;
            }
        }
        return count;
    }
};
