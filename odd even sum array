#include <bits/stdc++.h>
using namespace std;

int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */  
    int a;
    cin>> a;
    int arr[a];
    int even =0, odd =0;
    for(int i =0;i <a;i++){
        cin >> arr[i];
        if(arr[i] % 2 == 0){
            even += arr[i];
        }
        else{
            odd += arr[i];
        }
    } 
    cout << odd <<" " << even;
    
    return 0;
}
