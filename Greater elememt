int* greaterElement(int arr[], int n) {
    // Complete the function
    vector<int> v;
    unordered_map<int,int> m;
    for(int i=0;i<n;i++){
        v.push_back(arr[i]);
    }
    sort(v.begin(),v.end());
    for(int i=0;i<n-1;i++){
        m[v[i]] = v[i+1];
    }
    m[v[n-1]] = -10000000;
    for(int i=0;i<n;i++){
        arr[i] = m[arr[i]];
        
    }
    return arr;
}
