#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
	
    int n;
    scanf("%d", &n);
    //Complete the code to calculate the sum of the five digits on n.
    int temp=n;
    int digit =0;
    int s =0;
    while(temp> 0){
        digit = temp%10;
        s = s+ digit;
        temp = temp/10;
    }
    printf("%d",s);
    return 0;
}
