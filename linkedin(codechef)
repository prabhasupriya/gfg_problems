'''
# Node Class:
class Node:
    def init(self,val):
        self.data = val
        self.next = None
'''

class Solution:
    def removeDuplicates(self, head):
        if head is None:
            return head

        current = head
        while current.next is not None:
            if current.data == current.next.data:
                # Skip the duplicate node
                current.next = current.next.next
            else:
                current = current.next

        return head
