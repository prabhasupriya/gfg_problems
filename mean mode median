#include <bits/stdc++.h>
using namespace std;

int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */ 
    int a;
    cin>>a;
    vector<int> arr(a);
    int sum =0;
   
    for(int i=0;i<a;i++){
        cin >> arr[i];
        sum += arr[i];
    }  
    
    double mean = (double)sum / a;
    cout << fixed <<setprecision(2);
    cout <<mean<<" ";
    // median
    int k = a/2;
    sort(arr.begin() , arr.end());
    if(a%2 == 0){
     double   s = (arr[k]+ arr[k-1])/2.0;
     cout << fixed <<setprecision(2);
cout << s << " ";
    }
    else{
       double  s = arr[k];
       cout << fixed <<setprecision(2);
    cout << s<< " ";
    }
//mode
unordered_map<int ,int> freq;
for(int i : arr){
    freq[i]++;
}
int maxfreq=0;
int node = arr[0];
for(auto x : freq){
    if((x.second > maxfreq) || (x.first<node && x.second == maxfreq)){
        node = x.first;
        maxfreq = x.second;
    }
}
cout << fixed <<setprecision(1);
cout << node;
    
    return 0;
}
