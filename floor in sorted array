class Solution {
  public:
    int findFloor(vector<int>& arr, int x) {
        // code here
       // int maxq=-1;
        int index=-1;
        for(int i=0;i<arr.size();i++){
            if(arr[i] <= x){
                //maxq = max(maxq , arr[i]);
                index = i;
            }
        }
        return index;
    }
};
